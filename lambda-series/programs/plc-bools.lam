true = (/\ a -> ((\x -> \y -> x) : a -> a -> a)) : forall a . a -> a -> a
false = (/\ a -> (\x -> \y -> y) : a -> a -> a) : forall a . a -> a -> a
if = (/\ b -> (\bool -> \x -> \y -> (bool @b) x y)) : forall b. (forall a . (a -> a -> a)) -> b -> b -> b

